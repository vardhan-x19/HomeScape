<%- include('../partials/head') %>
<body class="p-10"  style="width: 100%; margin: 0; height: 100vh; display: flex; flex-direction: column;">
  <nav style="background-color: rgb(237, 36, 36); display: flex; justify-content: space-around;">
    <h5><a style="outline: none; text-decoration: none; color: white; font-size: medium;" href="/">Airbnb</a></h5>
    <h5><a style="outline: none; text-decoration: none; color: white; font-size: medium;" href="/home-lists">home-lists</a></h5>
    <h5><a style="outline: none; text-decoration: none; color: white; font-size: medium;" href="/home">register</a></h5>
    <h5><a style="outline: none; text-decoration: none; color: white; font-size: medium;" href="/cost">cost</a></h5>
    <h5><a style="outline: none; text-decoration: none; color: white; font-size: medium;" href="/location">location</a></h5>
    <h5><a style="outline: none; text-decoration: none; color: white; font-size: medium;" href="/host-homelists">Host-HomeLists</a></h5>
  </nav>
  <div id="centerDiv" style="flex-grow: 1; display: flex; align-items: center; justify-content: center; text-align: center;">
    <% if(data) { %>
      <div style="width: 350px; background: #fff; border-radius: 10px; box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1); overflow: hidden; text-align: left;">
       
        <div style="width: 100%; height: 300px; background: url('<%= data.imgUrl %>') center center/cover no-repeat;"></div>
        
        <!-- Content Section -->
        <div style="padding: 20px;">
          <h3 style="margin: 0 0 10px; color: #333;">House Name: <%= data.houseName %></h3>
          <p style="margin: 5px 0; color: #555;"><strong>Price:</strong> <%= data.price %></p>
          <p style="margin: 5px 0; color: #555;"><strong>House ID:</strong> <%= data.id %></p>
          <p style="margin: 5px 0; color: #555;"><strong>Location:</strong> <%= data.location %></p>
        </div>
        <%- include('../partials/fav')%>
      </div>
    <% } %>
  </div>
</body>
</html>
